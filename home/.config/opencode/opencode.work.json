{
  "$schema": "https://opencode.ai/config.json",
  "share": "disabled",
  "provider": {
    "anthropic": {
      "npm": "@ai-sdk/anthropic",
      "name": "Anthropic(Enterprise)",
      "options": {
        "baseURL": "{env:ANTHROPIC_BASE_URL}/v1",
        "headers": {
          "Authorization": "Bearer {env:ANTHROPIC_AUTH_TOKEN}"
        },
        "apiKey": ""
      },
      "models": {}
    },
    "anthropic-proxy": {
      "npm": "@ai-sdk/anthropic",
      "name": "Anthropic(Proxy)",
      "options": {
        "baseURL": "http://localhost:3001/v1",
        "headers": {
          "Authorization": "Bearer {env:ANTHROPIC_AUTH_TOKEN}"
        },
        "apiKey": ""
      },
      "models": {}
    },
    "openai": {
      "npm": "@ai-sdk/openai",
      "name": "OpenAI(Enterprise)",
      "options": {
        "baseURL": "{env:ANTHROPIC_BASE_URL}",
        "headers": {
          "Authorization": "Bearer {env:ANTHROPIC_AUTH_TOKEN}"
        },
        "apiKey": ""
      },
      "models": {}
    },
    "paypal": {
      "npm": "@paypalcorp/ai-gateway-ai-sdk",
      "name": "Cosmos",
      "models": {
        "gemini-2.0-flash": {
          "name": "2.0 Flash"
        },
        "gemini-2.5-flash": {
          "name": "2.5 Flash"
        },
        "gemini-2.5-pro": {
          "name": "2.5 Pro"
        },
        "gemini-3-pro-preview": {
          "name": "2.5 Pro"
        }
      }
    }
  },
  "disabled_providers": [
    "gemini",
    "opencode",
    "google-vertex",
    "vertex",
    "anthropic-vertex-ai",
    "google",
    "google-vertex-anthropic"
  ]
}
